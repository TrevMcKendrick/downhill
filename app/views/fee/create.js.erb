// Generated by CoffeeScript 1.6.2
<% if @fee.errors.count > 0 %>
    $('#fee_error_explanation').html("");
  <% @fee.errors.full_messages.each do |message| %>
    $('#fee_error_explanation').append("<%= message %>");
    $('#fee_error_explanation').append("<br>");    
  <% end %>
  document.getElementById("fee_error_explanation").style.display = 'block';
<% else %>
  document.getElementById("fee_error_explanation").style.display = 'none';
<% end %>