                    <div class="dt-wrapper">
                      <table id="dt_basic" class="table table-striped table-bordered table-hover dataTable" aria-describedby="dt_basic_info">
                      <thead>
                        <tr role="row">
                          <th class="sorting" role="columnheader" tabindex="0" aria-controls="dt_basic" rowspan="1" colspan="1" aria-label="Name: activate to sort column ascending" style="width: 150px;">Event</th>
                          <th class="sorting" role="columnheader" tabindex="0" aria-controls="dt_basic" rowspan="1" colspan="1" aria-label="Phone: activate to sort column ascending" style="width: 126px;">Date</th>
                          <th class="sorting" role="columnheader" tabindex="0" aria-controls="dt_basic" rowspan="1" colspan="1" aria-label="Company: activate to sort column ascending" style="width: 110px;">Tickets Sold</th>
                          <th class="sorting" role="columnheader" tabindex="0" aria-controls="dt_basic" rowspan="1" colspan="1" aria-label="Zip: activate to sort column ascending" style="width: 83px;">Published</th>
                          <th class="sorting" role="columnheader" tabindex="0" aria-controls="dt_basic" rowspan="1" colspan="1" aria-label="Zip: activate to sort column ascending" style="width: 83px;">Volunteer Leader</th>
                        </tr>
                      </thead>
                      <tbody role="alert" aria-live="polite" aria-relevant="all">
                      <% @events.each do |event| %> 
                          <tr class="<%= cycle("odd", "even") -%>">
                            <td class=" "><%= link_to event.title, edit_event_path(event) %></td>
                            <td class=" "><%= event.date %></td>
                            <td class=" "><%= event.users.count %></td>
                            <td class=" "><%= event.published %></td>
                            <td class=" "><%= event.volunteer_coordinator_email %></td>
                          </tr>
                      <% end %>
                      </tbody>
                    </table>
