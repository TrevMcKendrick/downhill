// Generated by CoffeeScript 1.6.2
<% if @wave.errors.count > 0 %>
    $('#wave_error_explanation').html("");
  <% @wave.errors.full_messages.each do |message| %>
    $('#wave_error_explanation').append("<%= message %>");
    $('#wave_error_explanation').append("<br>");    
  <% end %>
  document.getElementById("wave_error_explanation").style.display = 'block';
<% else %>
  document.getElementById("wave_error_explanation").style.display = 'none';
<% end %>