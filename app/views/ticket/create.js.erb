// Generated by CoffeeScript 1.6.2
<% if @ticket.errors.count > 0 %>
    $('#ticket_error_explanation').html("");
  <% @ticket.errors.full_messages.each do |message| %>
    $('#ticket_error_explanation').append("<%= message %>");
    $('#ticket_error_explanation').append("<br>");    
  <% end %>
  document.getElementById("ticket_error_explanation").style.display = 'block';
<% else %>
  document.getElementById("ticket_error_explanation").style.display = 'none';
<% end %>