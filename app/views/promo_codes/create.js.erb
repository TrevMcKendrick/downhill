// Generated by CoffeeScript 1.6.2
<% if @promo_code.errors.count > 0 %>
    $('#promo_code_error_explanation').html("");
  <% @promo_code.errors.full_messages.each do |message| %>
    $('#promo_code_error_explanation').append("<%= message %>");
    $('#promo_code_error_explanation').append("<br>");    
  <% end %>
  document.getElementById("promo_code_error_explanation").style.display = 'block';
<% else %>
  document.getElementById("promo_code_error_explanation").style.display = 'none';
<% end %>