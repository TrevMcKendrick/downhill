/*** #header ***/
#header {
	background: url(<%= asset_data_uri 'psd_html/bg-header.png' %>) repeat-y center center;
	background-size: 100% 1px;
	min-height: 46px;
	margin-bottom: 32px;
}

#header.no-bg {
	background: none;
}

#header.no-bg .wrapper {
	background-color: #cdd1da;

	position: relative;
	background: #cdd0d9;
	-webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
	-moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
	box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
	z-index: 3;
}

#header.no-bg .wrapper:before,
#header.no-bg .wrapper:after {
	content: "";
	position: absolute;
	z-index: -1;

	bottom: 28px;
	left: 10px;
	width: 98%;
	height: 10px;

	-webkit-box-shadow: 0 15px 10px rgba(0, 0, 0, 0.7);
	-moz-box-shadow: 0 15px 10px rgba(0, 0, 0, 0.7);
	box-shadow: 0 15px 10px rgba(0, 0, 0, 0.7);

	-webkit-transform: rotate(-3deg);
	-moz-transform: rotate(-3deg);
	-ms-transform: rotate(-3deg);
	-o-transform: rotate(-3deg);
	transform: rotate(-3deg);
}

#header.no-bg .wrapper:after {
	right : 10px;
	-webkit-transform: rotate(3deg);
	-moz-transform: rotate(3deg);
	-ms-transform: rotate(3deg);
	-o-transform: rotate(3deg);
	transform: rotate(3deg);
}

#header.no-bg img {
	width: 100%;
	height: auto;

	border: 3px solid #FFF;
}

#header .logo {
	background: #df5a49 url(<%= asset_data_uri 'psd_html/corner-header.png' %>) no-repeat bottom right;
	padding: 2px 0px 2px;
	width: 264px;
	height: 100%;
	float: left;
}

#header h1 {
	padding-left: 42px;
	font-size: 24px;
	font-family: 'sansationregular';
	text-transform: uppercase;
	font-weight: normal;

	background: url(<%= asset_data_uri 'psd_html/logo.png' %>) no-repeat top left;
}

#header h1 a {
	color: #fff;
}

#header .utility {
