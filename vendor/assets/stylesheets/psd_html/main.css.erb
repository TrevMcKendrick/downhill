	width: 30px;
	margin-top: -3px;
	padding-top: 29px;
	background-position: 0 -575px;
}

.event-form .action {
	margin-left: 186px;
}

.event-form .action button {
	background: url(<%= asset_data_uri 'psd_html/section-separator.png' %>) 0 -413px no-repeat;
	width: 180px;
	height: 47px;
	text-transform: uppercase;
	color: #fff;
	font-size: 15px;
	border: 0;
	padding-right: 44px;
}

.join-team,
.create-team {
	display: none;
	float: right;
	margin-right: 30px;
	position: relative;
}

.create-team input[type=text] {
	width: 285px;
}

.create-team input[type=text]:focus {
	border: 1px solid #d7d7d7;
}

.create-team:after {
	content: '';
	position: absolute;
	top: -11px;
	left: 160px;
	width: 17px;
	height: 12px;
	background: url(<%= asset_data_uri 'psd_html/section-separator.png' %>) no-repeat 0 -482px;
}

/*** Custom Select ***/
select {
	z-index: 4;
}

.customSelect {
	position: relative;
	height: 30px;
	border: 1px solid #d7d7d7;
	font-size: 16px;
	padding: 6px 5px 0 5px;
	background: url(<%= asset_data_uri 'psd_html/dropdown.png' %>) right center no-repeat;
	width: 378px;
}

.customSelect.customSelectOpen,
.customSelect.customSelectFocus {
	border: 1px solid #f6826b;
}

.customSelectInner {
	width: 100% !important;
}

.join-team .customSelect {
	width: 285px;
}

.join-team .customSelect:after {
	content: '';
	position: absolute;
	top: -11px;
	left: 37px;
	width: 17px;
	height: 12px;
	background: url(<%= asset_data_uri 'psd_html/section-separator.png' %>) no-repeat 0 -482px;
}

.join-team .customSelect.customSelectOpen,
.join-team .customSelect.customSelectFocus {
	border: 1px solid #d7d7d7;
}

/*** Custom Radio ***/
.radio label {
	text-align: left;
	width: auto;
	float: none;
	display: inline-block;
	cursor: pointer;
	position: relative;
	padding-left: 25px;
	margin-right: 15px;
	font-size: 16px;
}

input[type=radio] {
	display: none;
}

.radio label:before {
	content: "";
	display: inline-block;

	width: 14px;
	height: 14px;

	margin-right: 10px;
	position: absolute;
	left: 0;
	bottom: 3px;
	background-color: #fff;
	border: 1px solid #d9d9d9;
}

.radio label:before {
	border-radius: 8px;
}

.radio input[type=radio]:checked + label:before {
	content: "\2022";
	color: #f02e08;
	font-size: 40px;
	text-align: center;
	line-height: 15px;
}


/*** Footer ***/

#footer {
	background: #df5a49;
	padding: 12px 0;
	color: #fff;
	font-size: 11px;
}

#footer h1 {
	margin: 0;
	padding-left: 42px;
	font-size: 24px;
	font-family: 'sansationregular';
	text-transform: uppercase;
	font-weight: normal;
	background: url(<%= asset_data_uri 'psd_html/logo.png' %>) no-repeat top left;
}

#footer h1 a {
	color: #fff;
}

#footer .copy {
	display: block;
}

#footer p {
	margin: 0;
}

#footer .powered .left {
	margin-right: 10px;
	margin-top: 37px;
}

